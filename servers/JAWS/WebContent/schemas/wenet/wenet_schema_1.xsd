	<xsd:import namespace="http://www.it.ojp.gov/jxdm/3.0/proxy/dod_misc/1.0" schemaLocation="../3.0/proxy/dod_misc/1.0/dod_misc_full-doc.xsd"/>
	<xsd:import namespace="http://www.it.ojp.gov/jxdm/3.0/proxy/dod_exec-12958/1.0" schemaLocation="../3.0/proxy/dod_exec-12958/1.0/dod_exec-12958_full-doc.xsd"/>
	<xsd:import namespace="http://www.it.ojp.gov/jxdm/3.0/proxy/dod_jcs-pub2.0-misc/1.0" schemaLocation="../3.0/proxy/dod_jcs-pub2.0-misc/1.0/dod_jcs-pub2.0-misc_full-doc.xsd"/>
	<xsd:import namespace="http://www.it.ojp.gov/jxdm/3.0/proxy/fips_10-4/1.0" schemaLocation="../3.0/proxy/fips_10-4/1.0/fips_10-4_full-doc.xsd"/>
	<xsd:import namespace="http://www.it.ojp.gov/jxdm/3.0/proxy/fips_5-2/1.0" schemaLocation="../3.0/proxy/fips_5-2/1.0/fips_5-2_full-doc.xsd"/>
	<xsd:import namespace="http://www.it.ojp.gov/jxdm/3.0/proxy/fips_6-4/1.0" schemaLocation="../3.0/proxy/fips_6-4/1.0/fips_6-4_full-doc.xsd"/>
	<xsd:import namespace="http://www.it.ojp.gov/jxdm/3.0/proxy/iso_3166/1.0" schemaLocation="../3.0/proxy/iso_3166/1.0/iso_3166_full-doc.xsd"/>
	<xsd:import namespace="http://www.it.ojp.gov/jxdm/3.0/proxy/iso_639-2b/1.0" schemaLocation="../3.0/proxy/iso_639-2b/1.0/iso_639-2b_full-doc.xsd"/>
	<xsd:import namespace="http://www.it.ojp.gov/jxdm/3.0/proxy/iso_639-2t/1.0" schemaLocation="../3.0/proxy/iso_639-2t/1.0/iso_639-2t_full-doc.xsd"/>
	<xsd:import namespace="http://www.it.ojp.gov/jxdm/3.0/proxy/mn_offense/1.0" schemaLocation="../3.0/proxy/mn_offense/1.0/mn_offense_full-doc.xsd"/>

<?xml version='1.0' encoding='UTF-8'?>

<xsd:schema 
    attributeFormDefault='unqualified'
    elementFormDefault='qualified' 
    targetNamespace='http://www.it.ojp.gov/jxdm/3.0/proxy/ucr/1.0' 
    xmlns='http://www.it.ojp.gov/jxdm/3.0/proxy/ucr/1.0' 
    xmlns:j='http://www.it.ojp.gov/jxdm/3.0' 
    xmlns:ucr='http://www.it.ojp.gov/jxdm/ucr/1.0' 
    xmlns:xsd='http://www.w3.org/2001/XMLSchema'
  >

  <xsd:import schemaLocation='../../../jxdm_full-doc.xsd' namespace='http://www.it.ojp.gov/jxdm/3.0' />
  <xsd:import schemaLocation='../../../../ucr/1.0/ucr_full-doc.xsd' namespace='http://www.it.ojp.gov/jxdm/ucr/1.0' />

<!--Define Types-->
  <xsd:complexType name='OffenseCodeType'>
    <xsd:annotation>
      <xsd:documentation>A proxy code set that enumerates Uniform Crime Reporting (UCR) offense codes.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base='ucr:OffenseCodeType'>
          <xsd:attributeGroup ref='j:SuperTypeMetadata' />
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
</xsd:schema>

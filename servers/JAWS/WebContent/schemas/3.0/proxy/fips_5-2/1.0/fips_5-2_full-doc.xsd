<?xml version='1.0' encoding='UTF-8'?>

<xsd:schema 
    attributeFormDefault='unqualified'
    elementFormDefault='qualified' 
    targetNamespace='http://www.it.ojp.gov/jxdm/3.0/proxy/fips_5-2/1.0' 
    xmlns='http://www.it.ojp.gov/jxdm/3.0/proxy/fips_5-2/1.0' 
    xmlns:fips_5-2='http://www.it.ojp.gov/jxdm/fips_5-2/1.0' 
    xmlns:j='http://www.it.ojp.gov/jxdm/3.0' 
    xmlns:xsd='http://www.w3.org/2001/XMLSchema'
  >

  <xsd:import schemaLocation='../../../../fips_5-2/1.0/fips_5-2_full-doc.xsd' namespace='http://www.it.ojp.gov/jxdm/fips_5-2/1.0' />
  <xsd:import schemaLocation='../../../jxdm_full-doc.xsd' namespace='http://www.it.ojp.gov/jxdm/3.0' />

<!--Define Types-->
  <xsd:complexType name='USStateCodeType'>
    <xsd:annotation>
      <xsd:documentation>A proxy code list that enumerates states.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base='fips_5-2:USStateCodeType'>
          <xsd:attributeGroup ref='j:SuperTypeMetadata' />
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name='USStateNumericCodeType'>
    <xsd:annotation>
      <xsd:documentation>A proxy code list that enumerates states.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base='fips_5-2:USStateNumericCodeType'>
          <xsd:attributeGroup ref='j:SuperTypeMetadata' />
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
</xsd:schema>
